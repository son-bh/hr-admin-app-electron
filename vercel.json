{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|favicon\\.ico|robots\\.txt).*)",
      "destination": "/"
    },
    { "source": "/sw.js", "destination": "/sw.js" }
  ],
  "cleanUrls": true
}
